<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ContractId</fullName>
    <inlineHelpText>This field will be blank until a contract is generated by finance.</inlineHelpText>
    <lookupFilter>
        <active>true</active>
        <filterItems>
            <field>$Source.AccountId</field>
            <operation>equals</operation>
            <valueField>Contract.AccountId</valueField>
        </filterItems>
        <infoMessage>To manually select a contract that is related to the opportunity account, select from the available list.</infoMessage>
        <isOptional>true</isOptional>
    </lookupFilter>
    <trackFeedHistory>false</trackFeedHistory>
    <trackHistory>false</trackHistory>
    <type>Lookup</type>
</CustomField>
