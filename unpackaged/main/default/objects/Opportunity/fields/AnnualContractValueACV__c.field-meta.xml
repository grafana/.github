<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AnnualContractValueACV__c</fullName>
    <description>Pulls from the opportunity product object</description>
    <externalId>false</externalId>
    <formula>IF(
 (TermLength__c) &gt; 12,

( TotalContractValueTCV__c  / (TermLength__c)) * 12,

TotalContractValueTCV__c

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Value of the contract at an annual level, less services.</inlineHelpText>
    <label>Annual Contract Value (ACV)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
