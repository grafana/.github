<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_Logo_Amount__c</fullName>
    <description>This field is for the new logo bonus and combines Services and ACV for new business opps</description>
    <externalId>false</externalId>
    <formula>IF (ISPICKVAL(Type, "New Business"),

( AnnualContractValueACV__c  +  Services__c ),

0)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>New Logo Amount</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
