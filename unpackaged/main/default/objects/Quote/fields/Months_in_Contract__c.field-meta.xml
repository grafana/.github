<?xml version="1.0" encoding="utf-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Months_in_Contract__c</fullName>
    <description># of months in contract by Service Start Date to End date</description>
    <externalId>false</externalId>
    <formula>CEILING(( Service_End_Date__c - Service_Start_Date__c) / 365 * 12)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Months in Contract</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
